    "@types/react": "18.0.5",
    "@types/react-dom": "18.0.1",
    "@types/tailwindcss": "3.0.10",
    "@vitejs/plugin-react": "1.3.1",
    "autoprefixer": "10.4.4",
    "postcss": "8.4.12",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-icons": "4.3.1",
    "tailwindcss": "3.0.24",
    "typescript": "4.6.4",
    "vite": "2.9.5",
    "vite-tsconfig-paths": "3.4.1",
