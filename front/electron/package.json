{
  "name": "@shui/electron",
  "version": "0.0.0",
  "private": true,
  "main": "dist/main/index.cjs",
  "scripts": {
    "dev": "node scripts/watch.mjs",
    "build": "tsc -p src/renderer/tsconfig.json && node scripts/build.mjs && electron-builder",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ."
  },
  "devDependencies": {
    "eslint": "8.14.0",
    "@shui/ui": "workspace:*",
    "@shui/config": "workspace:*",
    "@types/node": "16.11.35",
    "@types/react": "18.0.5",
    "@types/react-dom": "18.0.1",
    "@vitejs/plugin-react": "1.3.1",
    "electron-builder": "23.0.3",
    "electron": "18.0.3",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-icons": "4.3.1",
    "sass": "1.50.0",
    "vite": "2.9.5",
    "vite-plugin-optimizer": "1.3.3",
    "typescript": "4.6.4",
    "rollup": "2.70.2",
    "tailwindcss": "3.0.24",
    "autoprefixer": "10.4.4",
    "postcss": "8.4.12",
    "asar": "3.1.0",
    "vite-tsconfig-paths": "3.4.1"
  },
  "eslintConfig": {
    "extends": "./node_modules/@shui/config/.eslintrc.cjs"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3000
  }
}
