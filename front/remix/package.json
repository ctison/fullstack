{
  "name": "@shui/remix",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf -- public/build/ && rushx build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o ./app/styles/app.css",
    "dev": "concurrently 'rushx dev:css' 'remix dev'",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o ./app/styles/app.css",
    "postinstall": "remix setup node && prisma generate",
    "lint": "eslint .",
    "test": "vitest"
  },
  "eslintIgnore": [
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@remix-run/react": "1.3.4",
    "@remix-run/serve": "1.3.4",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "remix": "1.3.4",
    "@prisma/client": "3.12.0",
    "@node-rs/bcrypt": "1.6.0",
    "tiny-invariant": "1.2.0",
    "marked": "4.0.12",
    "@mantine/ssr": "4.1.4",
    "@shui/ui": "workspace:*"
  },
  "devDependencies": {
    "@shui/config": "workspace:*",
    "vite-plugin-tsc": "0.1.1",
    "@babel/core": "7.17.9",
    "@testing-library/jest-dom": "5.16.3",
    "@remix-run/dev": "1.3.4",
    "@remix-run/eslint-config": "1.3.4",
    "@remix-run/node": "1.3.4",
    "@types/react": "18.0.5",
    "@types/react-dom": "18.0.1",
    "eslint": "8.14.0",
    "typescript": "4.6.4",
    "vitest": "0.8.2",
    "vite": "2.9.5",
    "@vitejs/plugin-react": "1.3.1",
    "vite-tsconfig-paths": "3.4.1",
    "happy-dom": "2.55.0",
    "tailwindcss": "3.0.24",
    "autoprefixer": "10.4.4",
    "postcss": "8.4.12",
    "prisma": "3.12.0",
    "concurrently": "7.1.0"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/@shui/config/.eslintrc.cjs"
    ]
  }
}
