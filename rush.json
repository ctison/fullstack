{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.70.0",
  "pnpmVersion": "7.1.0",
  "pnpmOptions": {
    "strictPeerDependencies": true,
    "preventManualShrinkwrapChanges": true,
    "useWorkspaces": true
  },
  "nodeSupportedVersionRange": "16",
  "ensureConsistentVersions": true,
  "projectFolderMaxDepth": 3,
  "projects": [
    {
      "packageName": "@shui/action-load-vars",
      "projectFolder": ".github/actions/load-vars"
    },
    {
      "packageName": "@shui/jspolicies",
      "projectFolder": "infra/jspolicies"
    },
    {
      "packageName": "@shui/config",
      "projectFolder": "libs/config"
    },
    {
      "packageName": "@shui/ui",
      "projectFolder": "front/ui"
    },
    {
      "packageName": "@shui/remix",
      "projectFolder": "front/remix"
    },
    {
      "packageName": "@shui/nextjs",
      "projectFolder": "front/nextjs"
    },
    {
      "packageName": "@shui/electron",
      "projectFolder": "front/electron"
    },
    {
      "packageName": "@shui/tauri",
      "projectFolder": "front/tauri"
    },
    {
      "packageName": "@shui/pulumi",
      "projectFolder": "infra/pulumi"
    },
    {
      "packageName": "@shui/playwright",
      "projectFolder": "qa/playwright"
    }
  ]
}
